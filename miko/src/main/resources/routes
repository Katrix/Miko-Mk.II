GET         /                                    miko.web.controllers.WebController.entry
GET         /selectGuild                         miko.web.controllers.WebController.selectGuild
GET         /codegrant                           miko.web.controllers.WebController.codeGrant
GET         /authenticate                        miko.web.controllers.WebController.authenticateOAuth(code: Option[String], state: Option[String])
GET         /logout                              miko.web.controllers.WebController.logout

GET         /guest                               miko.web.controllers.GuestController.guildHome
GET         /guest/help                          miko.web.controllers.GuestController.help
GET         /guest/music                         miko.web.controllers.GuestController.music
GET         /guest/settings                      miko.web.controllers.GuestController.settings
GET         /guest/log                           miko.web.controllers.GuestController.log

GET         /:guildId                            miko.web.controllers.WebController.guildHome(guildId: String)
GET         /:guildId/help                       miko.web.controllers.WebController.help(guildId: String)
GET         /:guildId/music                      miko.web.controllers.WebController.music(guildId: String)
GET         /:guildId/settings                   miko.web.controllers.WebController.settings(guildId: String)
POST        /:guildId/settings                   miko.web.controllers.WebController.submitSettings(guildId: String)
GET         /:guildId/log                        miko.web.controllers.WebController.log(guildId: String)

GET         /assets/*file                        controllers.Assets.versioned(path="/public", file)